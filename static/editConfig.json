{
  "debugLevel": 5,
  "filters": [
    {
      "file": "./usr/filters/testFilter.js",
      "name": "Filter"
    },
    {
      "file": "./usr/filters/doitourl.js",
      "name": "DOI to URL"
    },
    {
      "file": "./usr/filters/flickr.js",
      "name": "Flickr image"
    },
    {
      "file": "./usr/filters/gcgenerator.js",
      "name": "Generate GC"
    },
    {
      "file": "./usr/filters/hclust.js",
      "name": "Hierarchical clustering"
    },
    {
      "file": "./usr/filters/histogram.js",
      "name": "Color histogram"
    },
    {
      "file": "usr/filters/markdown",
      "name": "Convert Markdown to HTML"
    }
  ],
  "modules": {"folders": ["modules/types"]},
  "header": {
    "height": "30px",
    "elements": [
      {
        "type": "versionselector",
        "label": "Switch version",
        "url": "//www.lactame.com/visualizer/versions.json"
      },
      {
        "type": "couchdb2",
        "label": "Couch DB",
        "database": "c",
        "url": "{{ config.clp }}",
        "loginMethods": [
          "github",
          "google",
          "facebook",
          "couchdb"
        ]
      },
      {
        "type": "copyview",
        "label": "Copy view"
      },
      {
        "type": "pasteview",
        "label": "Paste view"
      },
      {
        "type": "blankview",
        "label": "Blank view"
      },
      {
        "type": "couchshare",
        "label": "Share view",
        "tinyUrl": "http://visualizer.epfl.ch/tiny",
        "couchUrl": "http://visualizer.epfl.ch",
        "database": "x"
      },
      {
        "type": "feedback",
        "label": "Feedback"
      }
    ]
  }
}